{
  "dependencies": {
    "axios": "^0.18.0",
    "date-fns": "^1.29.0",
    "marked": "^0.3.19",
    "material-ui": "^1.0.0-beta.41",
    "material-ui-icons": "^1.0.0-beta.36",
    "react": "^16.4.1",
    "react-bem-helper": "^1.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
